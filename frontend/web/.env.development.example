# Frontend Development Environment Configuration
# Copy this file to .env.development and update values for your machine

# API URL - Backend server address
# Windows local dev: http://localhost:3001/api
# Linux server: http://192.168.2.14:3001/api
VITE_API_URL=http://localhost:3001/api

# Vite dev server host
# Windows local dev: localhost
# Linux server: 192.168.2.14
VITE_HOST=localhost
